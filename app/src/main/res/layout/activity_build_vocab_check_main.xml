<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BuildVocabCheckMain">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="516dp"
        android:layout_height="1142dp"
        app:srcCompat="@drawable/buildvocabmainbg"
        tools:layout_editor_absoluteX="-53dp"
        tools:layout_editor_absoluteY="368dp" />

    <Spinner
        android:id="@+id/languageSpinner"
        android:layout_width="395dp"
        android:layout_height="51dp"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="16dp" />

    <Button
        android:id="@+id/buildVocabCheckBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="goToVocabTable"
        android:text="Build Vocabulary Quiz"
        tools:layout_editor_absoluteX="196dp"
        tools:layout_editor_absoluteY="113dp" />
</android.support.constraint.ConstraintLayout>